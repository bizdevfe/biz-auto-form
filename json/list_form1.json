[
  {
    "name": "bigImageList",
    "fieldType": "ListField",
    "label": "大图数",
    "length": [1,2,3,4],
    "content": [
      {
        "name": "image",
        "control": "ImageUpload",
        "label": "大图",
        "rules": {"imageRequired": true},
        "uploadRules": {
          "size": 100,
          "types": ["png"],
          "key": "750x320"
        }
      },
      {
        "name": "link",
        "control": "Input",
        "label": "大图链接",
        "defaultValue": "http://",
        "limiter": {"max": 512},
        "rules": {"required": true, "url": true}
      }
    ]
  },
  {
    "name": "countdownTime",
    "control": "DateTimeInput",
    "label": "设定时间",
    "rules": {"required": true}
  },
  {
    "name": "countdownText",
    "control": "Input",
    "label": "事件名称",
    "limiter": {"max": 24},
    "rules": {"required": true, "maxBytes": 24}
  },
  {
    "name": "buttonText",
    "control": "Input",
    "label": "按钮文字",
    "limiter": {"max": 14},
    "rules": {"required": true, "maxBytes": 14}
  },
  {
    "name": "buttonLink",
    "control": "Input",
    "label": "按钮链接",
    "defaultValue": "http://",
    "limiter": {"max": 512},
    "rules": {"required": true, "url": true}
  },
  {
    "name": "brandText",
    "control": "Input",
    "label": "品牌文字",
    "limiter": {"max": 12},
    "rules": {"required": true, "maxBytes": 12}
  },
  {
    "name": "brandLink",
    "control": "Input",
    "label": "品牌链接",
    "defaultValue": "http://",
    "limiter": {"max": 512},
    "rules": {"required": true, "url": true}
  }
]